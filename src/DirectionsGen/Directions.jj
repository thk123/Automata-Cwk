options {	STATIC = false;	IGNORE_CASE = true;
}PARSER_BEGIN(Directions)	class Directions	{		public static void main(String[] args) throws ParseException		{			Directions parser = new Directions(System.in);			parser.Start();		}	}PARSER_END(Directions)void Start() :{}{  ((PlaceDecleration() | PlaceRelation()) < LINETERMINATOR > (< WHITESPACE >)*)+ < EOF >}void PlaceDecleration() :{}{  "I see " < PLACEDEFINITION > (< LISTSEPERATOR > (< WHITESPACE >)* < PLACEDEFINITION >)*}void PlaceRelation() :{}{  < PLACEDEFINITION > (< WHITESPACE >) "is" (< WHITESPACE >) < DIRECTION > (< WHITESPACE >) "of" (< WHITESPACE >) < PLACEDEFINITION > (< WHITESPACE >)*}TOKEN :{  < DIRECTION : "NORTH" | "SOUTH" | "EAST" | "WEST" >}TOKEN :{  < LISTSEPERATOR : "," | "and" >}TOKEN :{  < PLACEDEFINITION : < PLACESTART > (< WHITESPACE >)+ < PLACE > >}TOKEN :{ < PLACESTART : "THE" > }TOKEN :{  < PLACE : "[" (["a" - "z"] | " ")+ "]" | (["a" - "z"])>}TOKEN :{  < WHITESPACE : ("\r" | "\n" | " ")+ >}TOKEN :{  < LINETERMINATOR : "." | ";" >}